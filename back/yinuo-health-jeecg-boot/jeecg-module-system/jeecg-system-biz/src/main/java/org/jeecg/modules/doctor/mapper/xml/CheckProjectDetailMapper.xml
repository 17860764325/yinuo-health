<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.doctor.mapper.CheckProjectDetailMapper">

	<delete id="deleteByMainId" parameterType="java.lang.String">
		DELETE 
		FROM  check_project_detail 
		WHERE
			 check_project_id = #{mainId} 	</delete>
	
	<select id="selectByMainId" parameterType="java.lang.String" resultType="org.jeecg.modules.doctor.entity.CheckProjectDetail">
		SELECT * 
		FROM  check_project_detail
		WHERE
			 check_project_id = #{mainId} 	</select>
</mapper>
